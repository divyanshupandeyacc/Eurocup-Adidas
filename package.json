{
  "name": "adidas-eurocup",
  "version": "1.0.0",
  "description": "This is adidas team formation web portal",
  "repository": {
    "type": "git",
    "url": "https://tools.adidas-group.com/bitbucket/projects/1/repos/test-app"
  },
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">= 12",
    "npm": ">= 6"
  },
  "scripts": {
    "start": "npm run build:dev",
    "clean": "npm run clean:dist && npm run clean:doc && npm run clean:test",
    "clean:dist": "rimraf dist",
    "clean:doc": "rimraf docs",
    "clean:test": "rimraf test/results",
    "build": "cross-env NODE_ENV=production webpack",
    "build:dev": "webpack-dev-server --open --hot",
    "lint": "npm run lint:src && npm run lint:style && npm run lint:test && npm run lint:base",
    "lint:base": "eslint *.js{,x}",
    "lint:src": "eslint src/",
    "lint:test": "eslint test/specs/",
    "lint:style": "stylelint src/**/*.scss",
    "doc": "jsdoc -r -R ./README.md -d docs -c ./jsdoc.config.js src/",
    "test": "npm run test:unit --env=jsdom",
    "test:unit": "jest --config jest.config.js --env=jsdom",
    "test:unit:watch": "npm run test:unit -- --watchAll",
    "test:unit:debug": "cross-env BABEL_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand --config jest.config.js"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-react": "7.14.5",
    "@babel/runtime": "7.12.1",
    "@testing-library/react": "^12.0.0",
    "build-tools-babel": "~1.2.0",
    "build-tools-babel-jest": "~1.2.0",
    "build-tools-common": "~1.0.2",
    "build-tools-javascript": "~1.2.0",
    "build-tools-jest": "~1.2.0",
    "build-tools-webpack": "~1.2.0",
    "build-tools-webpack-babel": "~1.0.1",
    "build-tools-webpack-files": "~1.2.0",
    "build-tools-webpack-sass": "~1.2.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-config-adidas-react": "1.3.2",
    "eslint-plugin-react": "7.24.0",
    "i18next": "19.8.3",
    "jest": "^27.1.0",
    "jest-dom": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "stylelint": "10.1.0",
    "stylelint-config-adidas": "1.3.0",
    "stylelint-config-adidas-bem": "1.2.0",
    "yarn-design-system": "^1.8.6",
    "yarn-design-system-fonts": "^1.2.2",
    "yarn-design-system-icons": "^1.2.4",
    "yarn-design-system-logos": "1.0.3",
    "yarn-design-system-react-components": "^1.16.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/user-event": "^13.2.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "enzyme": "^3.11.0",
    "eslint-plugin-prettier": "^3.4.0",
    "history": "^5.0.1",
    "jest-environment-jsdom": "^27.1.0",
    "prettier": "^2.3.2",
    "react-dotenv": "^0.1.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.1",
    "react-testing-library": "^8.0.1",
    "redux": "^4.1.1",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "swiper": "^6.5.9",
    "use-persisted-state": "^0.3.3",
    "yarn-design-system-react-select": "^1.1.4",
    "yarn-design-system-swiper": "^1.0.0"
  },
  "react-dotenv": {
    "whitelist": [
      "API_HOST",
      "TOKEN"
    ]
  }
}
